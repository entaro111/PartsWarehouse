@model IEnumerable<PartsWarehouse.Models.Dostawcy>

@{
    ViewBag.Title = "Dostawcy";
}

<h2>Dostawcy</h2>

<p>
    @Html.ActionLink("Stwórz nowego dostawcę", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nazwa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Miejscowosc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Kod)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nazwa)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Miejscowosc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Kod)
        </td>
        <td>
            @Html.ActionLink("Edycja", "Edit", new { id=item.Id_Dostawcy }) |
            @Html.ActionLink("Szczegóły", "Details", new { id=item.Id_Dostawcy }) |
            @Html.ActionLink("Usuń", "Delete", new { id=item.Id_Dostawcy })
        </td>
    </tr>
}

</table>
